{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.8",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.4.0",
    "typescript": "^5.4.3",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "build:test": "cross-env NODE_ENV=testing tsc && tsc-alias",
    "build": "tsc && tsc-alias",
    "clean": "rimraf dist",
    "clean:test": "cross-env NODE_ENV=testing rimraf dist-test",
    "dev": "yarn clean && yarn build && concurrently(\"tsc --watch\" \"tsc-alias -w\" \"nodemon dist/server.js\")",
    "test": "yarn clean:test && yarn build:test && cross-env NODE_ENV=testing jest --detectOpenHandles",
    "lint": "eslint . --ext .ts",
    "format": " prettier --write ."
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.3",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
